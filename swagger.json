            "type": "string",
            "description": "Mot de passe de connexion",
            "default": "1997@Arachides1997",
            "nullable": true
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "firstname": {
            "type": "string",
            "description": "Nom de l'utilisateur",
            "nullable": false,
            "default": "Libère"
          },
          "lastname": {
            "type": "string",
            "description": "Prénom de l'utilisateur",
            "default": "Huet"
          },
          "email": {
            "type": "string",
            "description": "Email de l'utilisateur",
            "nullable": false,
            "default": "Rjeanne30@hotmail.fr"
          },
          "picture": {
            "type": "number",
            "description": "Avatar de l'utilisateur"
          },
          "phone_number": {
            "type": "string",
            "description": "Numéro de téléphone",
            "nullable": false,
            "default": "+33 699844499"
          },
          "department": {
            "type": "object",
            "description": "Propriétaire de l'atelier",
            "default": ""
          },
          "password": {
            "type": "string",
            "description": "Mot de passe de connexion",
            "nullable": false,
            "default": "BIUFMRZ4bhAgmIt"
          },
          "roles": {
            "type": "string",
            "description": "OS de l'utilisateur",
            "nullable": true,
            "default": "editor"
          }
        },
        "required": [
          "firstname",
          "lastname",
          "email",
          "picture",
          "phone_number",
          "password",
          "roles"
        ]
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "firstname": {
            "type": "string",
            "description": "Nom de l'utilisateur",
            "nullable": false,
            "default": "Libère"
          },
          "lastname": {
            "type": "string",
            "description": "Prénom de l'utilisateur",
            "default": "Huet"
          },
          "email": {
            "type": "string",
            "description": "Email de l'utilisateur",
            "nullable": false,
            "default": "Rjeanne30@hotmail.fr"
          },
          "picture": {
            "type": "number",
            "description": "Avatar de l'utilisateur"
          },
          "phone_number": {
            "type": "string",
            "description": "Numéro de téléphone",
            "nullable": false,
            "default": "+33 699844499"
          },
          "department": {
            "type": "object",
            "description": "Propriétaire de l'atelier",
            "default": ""
          },
          "password": {
            "type": "string",
            "description": "Mot de passe de connexion",
            "nullable": false,
            "default": "BIUFMRZ4bhAgmIt"
          },
          "roles": {
            "type": "string",
            "description": "OS de l'utilisateur",
            "nullable": true,
            "default": "editor"
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {}
      },
      "AddCotationDto": {
        "type": "object",
        "properties": {
          "validate": {
            "type": "boolean",
            "description": "Statut de la cotation",
            "nullable": false,
            "default": false
          },
          "user": {
            "description": "Utilisateur",
            "allOf": [
              {
                "$ref": "#/components/schemas/User"
              }
            ]
          }
        },
        "required": [
          "validate",
          "user"
        ]
      },
      "AddHistoryDto": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Statut du courrier",
            "nullable": false,
            "default": "pending"
          },
          "user": {
            "description": "Utilisateur",
            "allOf": [
              {
                "$ref": "#/components/schemas/User"
              }
            ]
          }
        },
        "required": [
          "status",
          "user"
        ]
      },
      "CreateCourrierDto": {
        "type": "object",
        "properties": {
          "objet": {
            "type": "string",
            "description": "Demande de partenariat",
            "nullable": false
          },
          "picture": {
            "type": "string",
            "description": "Nom de l'utilisateur",
            "nullable": true,
            "default": null
          },
          "emetteur": {
            "type": "string",
            "description": "Port autonome de Douala",
            "nullable": false
          },
          "code": {
            "type": "string",
            "description": "Code du courrier",
            "nullable": false
          },
          "recepteur": {
            "type": "string",
            "description": "Fast General Trading",
            "nullable": false
          },
          "direction": {
            "type": "string",
            "description": "Département récepteur du courrier",
            "nullable": false,
            "default": "fgt"
          },
          "status": {
            "type": "string",
            "description": "Statut du courrier",
            "nullable": false,
            "default": "pending"
          },
          "type": {
            "type": "string",
            "description": "Type du courrier",
            "nullable": false,
            "default": "entrant"
          },
          "category": {
            "type": "string",
            "description": "Catégorie du courrier",
            "nullable": false,
            "default": "courrier"
          },
          "cotation": {
            "description": "Cotation du courrier",
            "default": [],
            "allOf": [
              {
                "$ref": "#/components/schemas/AddCotationDto"
              }
            ]
          },
          "modifications_history": {
            "description": "Historique du courrier",
            "default": [],
            "allOf": [
              {
                "$ref": "#/components/schemas/AddHistoryDto"
              }
            ]
          },
          "documents_annexe": {
            "description": "Image de l'atelier",
            "default": [],
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "objet",
          "emetteur",
          "code",
          "recepteur",
          "direction",
          "status",
          "type",
          "category"
        ]
      },
      "UpdateCourrierDto": {
        "type": "object",
        "properties": {
          "objet": {
            "type": "string",
            "description": "Demande de partenariat",
            "nullable": false
          },
