    "/files/single-file": {
      "post": {
        "operationId": "uploadFile",
        "summary": "Upload d'un fichier",
        "description": "Upload d'un fichier sur le serveur",
        "parameters": [
          {
            "name": "type",
            "required": true,
            "in": "query",
            "schema": {
              "enum": [
                "COURRIER_VIDEO",
                "COURRIER_IMAGE",
                "COURRIER_IMAGE_ANNEXE",
                "COURRIER_DOCUMENT",
                "OTHER"
              ],
              "type": "string"
            }
          },
          {
            "name": "entity",
            "required": true,
            "in": "query",
            "schema": {
              "enum": [
                "COURRIER"
              ],
              "type": "string"
            }
          },
          {
            "name": "filetype",
            "required": true,
            "in": "query",
            "schema": {
              "enum": [
                "PHOTO",
                "VIDEO",
                "PDF",
                "DOC"
              ],
              "type": "string"
            }
          },
          {
            "name": "courrierId",
            "required": false,
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "role",
            "required": true,
            "in": "query",
            "schema": {
              "enum": [
                "admin",
                "standard",
                "assistante_dg",
                "directeur_general_adjoint",
                "editor",
                "directeur_departement"
              ],
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Succes upload fichier",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateFileDto"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal server error"
          }
        },
        "tags": [
          "File Endpoint"
        ],
        "security": [
          {
            "bearer": []
          }
        ]
      }
    },
